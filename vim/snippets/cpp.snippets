	const int N = 10101;
	const int inf = 1 << 20 ;

	struct edge{
		int v, w;

		edge( int iv = 0  , int iw = 0 ) : v(iv) , w(iw) {}
	};

	struct singlePath{
		int v,w;

		singlePath( int iv = 0 , int iw = 0 ) : v(iv) , w(iw) {}

		const bool operator < (const singlePath rhs ) const {
			return w > rhs.w;
		}
	};

	int n,e;
	int source,  sink;
	vector< edge > list[N];
	int dist[N];
	int parent[N];
	bool visited[N];

	priority_queue< singlePath > heap;

	void Dijkstra( int source, int sink )
	{
		for(int i=1;i<=n;i++)
		{
			dist[i] = inf;
			visited[i] = false;
			parent[i] = 0;
		}

		dist[source] = 0;
		parent[source] = source;

		heap.push( singlePath( source, dist[source] ) );

		while ( !heap.empty() )
		{
			int u = heap.top().v;
			heap.pop();
			visited[u] = true;

			int size=list[u].size();
			for(int i=0;i<size;i++)
			{
				int v = list[u][i].v;
				int w = list[u][i].w;

				if ( dist[u] == inf || visited[v] )
					continue;

				if ( dist[u] + w <  dist[v] )
				{
					parent[v] = u;
					dist[v] = dist[u] + w;
					heap.push( singlePath( v, dist[v] ) );
				}
			}
		}
	}
